
.processc {
  flex: 1;
  font-size: small;
  line-height: 30px;
  margin-left: 20px;
}

.line {
  width: 10px;
  height: 50%;
  border-right: solid #e5e5e5 1.5px;
}

.history {
  display: flex;
  flex-direction: row;
  width: 100%;
  position: relative;
  background-color: #fff;
  border-radius: 3%;
  margin-bottom: 1.5px; 
}
.left {
  height: 100%;
  margin-left: 20px;
  display: flex;
  flex-direction: column;
}
.right {
  height: 50px;
  margin-top: 8px;
  margin-left: 20px;
  margin-bottom: 10px;
  display: flex;
  flex-direction: column;
}